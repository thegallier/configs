shakti_to_python = {
    '+': {
        'desc': 'addition',
        'python': 'np.add(a, b)',
    },
    '-': {
        'desc': 'subtraction',
        'python': 'np.subtract(a, b)',
    },
    '*': {
        'desc': 'multiplication',
        'python': 'np.multiply(a, b)',
    },
    '%': {
        'desc': 'division',
        'python': 'np.divide(a, b)',
    },
    'til': {
        'desc': 'sequence generation',
        'python': 'np.arange(n)',
    },
    'max': {
        'desc': 'maximum',
        'python': 'np.max(a)',
    },
    'min': {
        'desc': 'minimum',
        'python': 'np.min(a)',
    },
    'avg': {
        'desc': 'average',
        'python': 'np.mean(a)',
    },
    'count': {
        'desc': 'count',
        'python': 'len(a)',
    },
    'where': {
        'desc': 'filter',
        'python': 'a[a > n]',
    }
}

